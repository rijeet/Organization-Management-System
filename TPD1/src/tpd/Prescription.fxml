<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="prescribePane" prefHeight="700.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tpd.PrescriptionController">
   <children>
      <VBox alignment="CENTER" prefHeight="145.0" prefWidth="1080.0" style="-fx-background-color: #09689e;">
         <children>
            <Label contentDisplay="CENTER" text="Prescription" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="46.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Label layoutX="109.0" layoutY="253.0" text="UserName :" />
      <Label layoutX="109.0" layoutY="315.0" text="Problem : " />
      <Label layoutX="109.0" layoutY="370.0" text="Solution : " />
      <TextField fx:id="username" layoutX="219.0" layoutY="249.0" />
      <TextField fx:id="problem" layoutX="221.0" layoutY="311.0" />
      <TextField fx:id="solution" layoutX="219.0" layoutY="370.0" prefHeight="119.0" prefWidth="221.0" />
      <Button layoutX="359.0" layoutY="516.0" mnemonicParsing="false" onAction="#SaveAction" prefHeight="31.0" prefWidth="81.0" style="-fx-background-color: #09689e;" text="Send" textFill="WHITE">
         <font>
            <Font name="Baskerville Old Face" size="16.0" />
         </font>
      </Button>
      <Button layoutX="255.0" layoutY="518.0" mnemonicParsing="false" onAction="#Reset_Action" prefHeight="31.0" prefWidth="81.0" style="-fx-background-color: #09689e;" text="Reset" textFill="WHITE">
         <font>
            <Font name="Baskerville Old Face" size="16.0" />
         </font>
      </Button>
      <Button layoutX="31.0" layoutY="641.0" mnemonicParsing="false" onAction="#BackAction" prefHeight="31.0" prefWidth="81.0" style="-fx-background-color: #09689e;" text="Back" textFill="WHITE">
         <font>
            <Font name="Baskerville Old Face" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
