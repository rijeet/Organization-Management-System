            <Font name="Baskerville Old Face" size="16.0" />
         </font>
      </Button>
      <Button fx:id="Patient_List_Button" layoutX="41.0" layoutY="285.0" mnemonicParsing="false" onAction="#Show_ALL_patient" prefHeight="37.0" prefWidth="129.0" text="Show All Patient" />
      <HBox fx:id="Patient_List_Hbox" layoutX="198.0" layoutY="230.0" prefHeight="485.0" prefWidth="868.0">
