   <children>
      <VBox alignment="CENTER" prefHeight="145.0" prefWidth="1080.0" style="-fx-background-color: #09689e;">
         <children>
            <ImageView fitHeight="150.0" fitWidth="1059.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Image/logo.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
      <Button fx:id="BackButton" layoutX="41.0" layoutY="666.0" mnemonicParsing="false" onAction="#Back_Button_action" style="-fx-background-color: #09689e;" text="Back" textFill="WHITE">
